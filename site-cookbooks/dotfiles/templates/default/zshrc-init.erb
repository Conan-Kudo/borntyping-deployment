# Configure oh-my-zsh
ZSH=$HOME/.oh-my-zsh
ZSH_THEME='borntyping'
DISABLE_AUTO_UPDATE='true'
COMPLETION_WAITING_DOTS='true'

# Initialise oh-my-zsh
source $ZSH/oh-my-zsh.sh

# Override oh-my-zsh settings
setopt AUTO_NAME_DIRS
setopt AUTO_PUSH_D
setopt NO_AUTO_CD
setopt INTERACTIVE_COMMENTS
unsetopt CORRECT
unsetopt CORRECTALL
